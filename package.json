{
  "name": "serverless-graphql-typescript",
  "description": "A starting template for creating Serverless and TypeScript.",
  "author": "Eduard Castellanos",
  "license": "Apache-2.0",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git@github.com:EduardMcfly/serverless-typescript-template.git"
  },
  "scripts": {
    "test": "jest --no-cache --coverage",
    "start": "serverless offline start",
    "deploy-prod": "serverless --stage=production deploy"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "./node_modules/ts-jest/preprocessor.js",
      "\\.(gql|graphql)$": "jest-transform-graphql"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts?)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "mapCoverage": true
  },
  "dependencies": {
    "apollo-server-lambda": "^2.17.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "graphql-tools": "^4.0.0",
    "jest-transform-graphql": "^2.1.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.62",
    "@types/graphql": "^14.5.0",
    "@types/node": "^14.11.1",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "aws-sdk": "^2.814.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "eslint": "^7.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "serverless-offline": "^6.7.0",
    "serverless-webpack": "^5.3.5",
    "ts-jest": "^26.4.0",
    "ts-loader": "^8.0.4",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-node-externals": "^2.5.2"
  }
}
